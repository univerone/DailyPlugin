{
  "name": "WordsPerDay",
  "version": "1.0.0",
  "description": "wechaty每日一句插件",
  "main": "dist/src/index.js",
  "typings": "dist/src/index.d.ts",
  "scripts": {
    "clean": "shx rm -fr dist/*",
    "dist": "npm run clean && tsc",
    "pack": "npm pack",
    "lint": "npm run lint:es && npm run lint:ts && npm run lint:md",
    "lint:md": "markdownlint README.md",
    "lint:ts": "tsc --noEmit",
    "example": "ts-node src/ding-dong-bot.ts",
    "start": "npm run example",
    "test": "npm run lint && npm run test:unit",
    "test:pack": "bash -x scripts/npm-pack-testing.sh",
    "test:unit": "blue-tape -r ts-node/register \"src/**/*.spec.ts\" \"src/*.spec.ts\" \"tests/*.spec.ts\" \"tests/**/*.spec.ts\"",
    "lint:es": "eslint --ignore-pattern tests/fixtures/ '{bin,examples,scripts,src,tests}/**/*.ts'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/univerone/WordsPerDay.git"
  },
  "keywords": [],
  "author": "univerone",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/univerone/WordsPerDay/issues"
  },
  "homepage": "https://github.com/univerone/WordsPerDay#readme",
  "devDependencies": {
    "@chatie/eslint-config": "^0.8.1",
    "@chatie/git-scripts": "^0.6.2",
    "@chatie/semver": "^0.4.7",
    "@chatie/tsconfig": "^0.10.1",
    "@types/request": "^2.48.5",
    "@typescript-eslint/eslint-plugin": "^3.6.0",
    "eslint": "^7.4.0",
    "shx": "^0.3.2",
    "tstest": "^0.4.10",
    "typescript": "^3.9.6",
    "wechaty-puppet": "^0.26.2",
    "wechaty-puppet-hostie": "^0.8.8",
    "wechaty-puppet-padplus": "^0.7.21",
    "wechaty-puppet-puppeteer": "^0.23.1"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "gm": "^1.23.1",
    "memory-card": "^0.6.21",
    "qrcode-terminal": "^0.12.0",
    "wechaty": "^0.40.10",
    "wechaty-plugin-contrib": "^0.10.6"
  }
}
